<%= search_form_for(@q , url: search_path)  do |f| %>
  <p><%= f.text_field  :title_cont , placeholder:"Search for a collectible", class:"form-control form-inline" %></p>
  <%= f.submit class:"btn btn-primary" %>
  <%= link_to 'Clear search', search_path %>
  <%= link_to 'Go To Current Listings', listings_path %>
<% end %>

 