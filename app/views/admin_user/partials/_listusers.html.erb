<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Username</th>
      <th scope="col">Email</th>
      <th scope="col">Account Deleted Date</th>
      <th scope="col">Edit User</th>
    </tr>
  </thead>
  <tbody>
  <% list_all_users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <% if user.deleted_at.nil? == false %>
      <td><%= user.deleted_at %></td>
      <% else %>
      <td></td>
    <% end %>
    
    <td><%= link_to 'Edit User', user_edit_path(user) %></td>
  </tr>
  <% end %>
  </tbody>
</table>